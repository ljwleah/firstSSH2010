<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- school.hbm.xml -->
<hibernate-mapping>
  <class name="willow.entity.code.School" table="school" catalog="hawk">
    <id name="id" type="java.lang.String">
      <column name="id" length="20" />
      <generator class="assigned" />
    </id>
    <property name="upid" type="java.lang.String">
      <column name="upid" length="20" />
    </property>
    <property name="name" type="java.lang.String">
      <column name="name" length="0" not-null="true" />
    </property>
    <property name="abb" type="java.lang.String">
      <column name="abb" length="10" />
    </property>
    <property name="type" type="java.lang.String">
      <column name="type" length="10" />
    </property>
  </class>
</hibernate-mapping>
